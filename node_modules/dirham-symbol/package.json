{
  "name": "dirham-symbol",
  "version": "4.0.4",
  "description": "A React component that provides the new UAE Dirham (AED) symbol using a custom font. This package makes it easy to display the official Dirham symbol in your React applications with full customization support.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "README.md"
  ],
  "style": "src/styles/index.css",
  "type": "commonjs",
  "keywords": [
    "uae",
    "dirham",
    "aed",
    "currency",
    "symbol",
    "react",
    "component",
    "font",
    "unicode",
    "emirates"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prasathmani/dirham-symbol.git"
  },
  "homepage": "https://github.com/prasathmani/dirham-symbol#readme",
  "bugs": {
    "url": "https://github.com/prasathmani/dirham-symbol/issues"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Prasath Mani",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
